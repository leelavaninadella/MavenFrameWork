<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="IteraQA Suite">
	<parameter name="BrowserName" value="edge"/>
	<parameter name="url"
		value="https://itera-qa.azurewebsites.net/home/automation" />
	<parameter name="sleepCount" value="5000" />
	<test thread-count="5" name="CheckBox Test" enabled="false">
		<classes>
			<class name="com.ackage.iteraqa.CheckBox1">
			<methods>
			<include name="launchApp" /> 
			<include name="clickOnCheckBox" /> 
			</methods>

			</class>
		</classes>
	</test> <!-- Test -->
	
	<test thread-count="5" name="Last CheckBox Test">
		<classes>
			<class name="com.ackage.iteraqa.CheckBox1">
			<methods>
			<include name="launchApp" /> 
			<include name="clickOnLast3CheckBox" /> 
			</methods>

			</class>
		</classes>
	</test> <!-- Test -->
	
	<test thread-count="5" name="First CheckBox Test">
		<classes>
			<class name="com.ackage.iteraqa.CheckBox1">
			<methods>
			<include name="launchApp" /> 
			<include name="clickOnFirst3CheckBoxes" /> 
			</methods>

			</class>
		</classes>
	</test> <!-- Test -->
	
	
</suite> <!-- Suite -->
